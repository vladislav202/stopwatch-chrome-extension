{
   "background": {
      "persistent": true,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_title": "stopwatch",
      "default_icon": "stopwatch.png"
   },
   "content_scripts": [
      {
         "matches": [ "<all_urls>" ],
         "css": [ "css/content.css" ],
         "js": [ "js/content.js" ]
      }
   ],
   "web_accessible_resources": [
      "css/content.css",
      "js/content.js"
   ],
   "description": "stopwatch",
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "22",
   "name": "Stopwatch For Chrome",
   "permissions": [ "<all_urls>", "tabs", "storage" ],
   "short_name": "Stopwatch",
   "version": "1.0.0"
}
